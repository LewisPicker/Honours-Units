back into our simplified metric to get.\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["ds", "2"], "  ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox["e", 
        RowBox[{"2", 
         RowBox[{"\[Alpha]", "(", "r", ")"}]}]]}], 
      SuperscriptBox["dt", "2"]}], "+", 
     RowBox[{
      SuperscriptBox["e", 
       RowBox[{"2", 
        RowBox[{"\[Beta]", "(", "r", ")"}]}]], 
      SuperscriptBox["dr", "2"]}], "+", 
     RowBox[{
      SuperscriptBox["r", "2"], 
      SuperscriptBox["d\[CapitalOmega]", "2"]}]}]}], TraditionalForm]],
  ExpressionUUID->"57b2a527-3d45-4225-b3c6-c6b92f34d385"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["ds", "2"], "  ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", 
        RowBox[{"1", " ", "-", 
         FractionBox[
          SubscriptBox["R", "s"], "r"]}], ")"}]}], 
      SuperscriptBox["dt", "2"]}], "+", 
     RowBox[{
      SuperscriptBox["e", 
       RowBox[{
        RowBox[{"-", "2"}], "a"}]], 
      SuperscriptBox["dr", "2"]}], "+", 
     RowBox[{
      SuperscriptBox["r", "2"], 
      SuperscriptBox["d\[CapitalOmega]", "2"]}]}]}], TraditionalForm]],
  ExpressionUUID->"a96f59da-fbb7-4bc0-98ec-2e78da6b3d90"],
