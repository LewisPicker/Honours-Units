  FormBox[
   RowBox[{
    SuperscriptBox["ds", "2"], "  ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", 
        RowBox[{"1", " ", "-", 
         FractionBox[
          SubscriptBox["R", "s"], "r"]}], ")"}]}], 
      SuperscriptBox["dt", "2"]}], "+", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", " ", "-", 
         FractionBox[
